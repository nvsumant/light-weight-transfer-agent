    <!--Resources that requires-->
    <resources>
        <j2se version="1.6+" href="http://java.sun.com/products/autodl/j2se"/>
        <jar href="LightWeightTransferAgent.jar" main="true"/>
        <jar href="lib/jftp.jar"/>
        <jar href="lib/junit.jar"/>
        <jar href="lib/log4j-1.2.16.jar"/>
    </resources>
    <application-desc main-class="lightweighttransferagent.Main">
    </application-desc>
</jnlp>
