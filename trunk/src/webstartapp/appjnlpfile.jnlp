<!--FNLP file that describe how appication should be launched-->
<?xml version="1.0" encoding="UTF-8"?>
<jnlp codebase="file:/media/Other/LightWeightTransferAgent/dist/" href="appjnlpfile.jnlp" spec="1.0+" >
    <!--Informaton about application-->
    <information>
        <title>Light Weight Transfer Agent</title>
        <vendor>Diluka</vendor>
        <offline-allowed/>
    </information>
    <!--Security Details-->
    <security>
        <all-permissions/>
